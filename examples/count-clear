;   let i = 0
;   let sbl = 2000
;   while i < sbl
;       let target = sbl + i
;       *target = '+'
;       i += 1
;   i = r1
;   sbl = r2
;   target = r3

; 64 let i = 0
mov r1 0

; 67 let sbl = 2000
mov r2 2000

; 70 let target = sbl
mov ra r2

; 73 target += i
add ra r1

; 76 *target = '+'
mov r3 43
store ra r3

; 82 i += 1
mov ra 1
add r1 ra

; 88: while i < sbl
mov ra r2
sub ra r1
jnz &70 ra

; 96: halt
jmp &0x8000

